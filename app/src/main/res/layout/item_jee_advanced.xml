<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="@dimen/_16sp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/clBatchImageRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/leafImageLeftRight"
            android:layout_width="57dp"
            android:layout_height="85dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/group_1171276773"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tvtext500Right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:gravity="end"
            android:text="767"
            android:textColor="#2B2829"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/leafImageRightRight"
            app:layout_constraintStart_toStartOf="@id/leafImageLeftRight"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tvJEEMainsRight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="Selections"
            android:textAlignment="center"
            android:textColor="#898384"
            android:layout_marginTop="@dimen/_5dp"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/tvtext500Right"
            app:layout_constraintStart_toEndOf="@id/leafImageLeftRight"
            app:layout_constraintStart_toStartOf="@id/tvtext500Right"
            app:layout_constraintTop_toBottomOf="@id/tvtext500Right" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/tvBatchYearRight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_5dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/year_bg"
            android:fontFamily="@font/nunito"
            android:paddingHorizontal="8dp"
            android:text="2024"
            android:textAlignment="center"
            android:textColor="#5757F7"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/tvJEEMainsRight"
            app:layout_constraintStart_toStartOf="@+id/tvJEEMainsRight"
            app:layout_constraintTop_toBottomOf="@id/tvJEEMainsRight" />

        <ImageView
            android:id="@+id/leafImageRightRight"
            android:layout_width="57dp"
            android:layout_height="85dp"
            android:layout_marginStart="42dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/group_1171276772"
            app:layout_constraintStart_toStartOf="@id/tvtext500Right"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>