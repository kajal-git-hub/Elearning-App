<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/_10dp"
    android:layout_margin="@dimen/_10dp"
    android:background="@drawable/card_bg">

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_bg_c2e7bf"
        android:gravity="center"
        android:padding="@dimen/_10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/star"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerInParent="true"
            android:src="@drawable/ic_test_star_cffca88"
            app:tint="@color/c_c2e7bf" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/shift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/star"
            android:layout_centerInParent="true"
            android:fontFamily="@font/nunito"
            android:text="Shift 1"
            android:textColor="@color/_357f2f"
            android:textStyle="normal" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/year"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/shift"
            android:layout_centerInParent="true"
            android:fontFamily="@font/nunito"
            android:text="2023"
            android:textColor="@color/_357f2f"
            android:textStyle="bold" />
    </RelativeLayout>

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/materialTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10dp"
        android:fontFamily="@font/nunito"
        android:textColor="@color/black"
        android:text="JEE Mains Paper With \nSolutions - 24 Jun, 23"
        app:layout_constraintStart_toEndOf="@+id/relativeLayout2"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/materialTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10dp"
        android:layout_marginTop="@dimen/_10dp"
        android:fontFamily="@font/nunito"
        android:text="Resume from where you left off"
        android:textColor="@color/_726c6c"
        app:layout_constraintBottom_toBottomOf="@+id/relativeLayout2"
        app:layout_constraintStart_toEndOf="@+id/relativeLayout2"
        app:layout_constraintTop_toBottomOf="@+id/materialTextView" />

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_2dp"
        android:layout_marginTop="@dimen/_12dp"
        android:background="@drawable/dottedline"
        app:layout_constraintTop_toBottomOf="@+id/materialTextView2"
        tools:layout_editor_absoluteX="10dp" />

    <com.google.android.material.button.MaterialButton
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_12dp"
        android:backgroundTint="@color/white"
        app:strokeWidth="@dimen/_1dp"
        android:fontFamily="@font/nunito"
        app:strokeColor="@color/blue_3E3EF7"
        android:text="Contact to resume test"
        android:textColor="@color/blue_3E3EF7"
        app:iconSize="14dp"
        app:iconTint="@null"
        app:iconGravity="textStart"
        android:textStyle="normal"
        app:icon="@drawable/call_calling"
        app:cornerRadius="@dimen/_8dp"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

</androidx.constraintlayout.widget.ConstraintLayout>