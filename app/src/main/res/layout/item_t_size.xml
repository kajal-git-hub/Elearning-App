<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="48dp"
    android:layout_margin="6dp"
    android:id="@+id/getStarted_bg_layoutSecond"
    android:background="@drawable/getstarted_itembg_unselected"
    android:paddingStart="17dp"
    android:paddingTop="12dp"
    android:paddingBottom="12dp">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/Select_Size"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/property_default"
        android:drawablePadding="12dp"
        android:fontFamily="@font/nunito"
        android:text="Small (S)"
        android:textColor="#433E3F"
        android:textSize="@dimen/_16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
